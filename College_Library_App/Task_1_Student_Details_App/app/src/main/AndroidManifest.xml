<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.studentsdetails">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Students_Details_App"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InternshipNewFirebase">
        <activity android:name="com.example.studentsdetails.MainActivity3" />
        <activity
            android:name="com.example.studentsdetails.StudentsInfo"
            tools:ignore="Instantiatable" />
        <activity android:name="com.example.studentsdetails.MainActivity2" />
        <activity android:name="com.example.studentsdetails.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>